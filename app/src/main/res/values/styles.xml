<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">#1E32FA</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:statusBarColor">#1E32FA</item>
    </style>
    <style name="OnBoardingButton">
        <item name="android:background">@drawable/button_main</item>
    </style>
    <style name="OnBoardingButtonEnable">
        <item name="android:background">@drawable/button_enable</item>
    </style>
    <style name="LayoutTopShape">
        <item name="android:background">@drawable/layout_shape</item>
    </style>
</resources>